        <Literal>a component produced by one division of a manufacturer and consumed in another</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="iof-av:firstOrderLogicDefinition"/>
        <IRI>RawMaterialRole</IRI>
        <Literal>RawMaterialRole(x) → Role(x) ∧ ∃a,∃b,∃r, ∃z(BuyingBusinessProcess(a)  ∧ BusinessOrganization(b) ∧ ManufacturerRole(r) ∧MaterialEntity(z) ∧ roleOf(r,b) ∧ hasParticipantAtSomeTime(a,b) ∧ hasParticipantAtSomeTime(a,z))</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="iof-av:firstOrderLogicDefinition"/>
        <IRI>RawMaterialRole</IRI>
        <Literal>Role(x) ∧ ∃p,∃a,∃b,∃y,∃r, ∃z(ProductProductionProcess(p) ∧ BuyingBusinessProcess(a)  ∧ BusinessOrganization(b) ∧ MaterialProduct(y) ∧ ManufacturerRole(r) ∧MaterialEntity(z) ∧ roleOf(r,b) ∧ hasParticipantAtSomeTime(a,b) ∧ hasParticipantAtSomeTime(a,z) ∧ isInputOf(z,p) ∧ hasOutput(p,y) ∧ continuantPartOfAtAllTimes(z,y) ∧ roleOf(x,z)) → RawMaterialRole(x)</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
