        <IRI>PlannedProcess</IRI>
        <Literal>PlannedProcess(x) ↔ Process(x) ∧ ∃s (PlanSpecification(s) ∧ prescribes(s, x))</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="iof-av:naturalLanguageDefinition"/>
        <IRI>PlannedProcess</IRI>
