    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>PlanSpecification</IRI>
        <Literal>When concretized, plan specification may be realized in a process performed by some agent to achieve the prescribed process objectives by taking the prescribed actions.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
