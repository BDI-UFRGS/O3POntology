        <Literal>a person&apos;s name designates that person; a URL designates the location of a Web Page on the internet; a vehicle identification number designates some vehicle</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="iof-av:adaptedFrom"/>
        <IRI>designates</IRI>
        <Literal datatypeIRI="http://www.w3.org/2001/XMLSchema#anyURI">http://www.ontologyrepository.com/CommonCoreOntologies/Mid/InformationEntityOntology</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="iof-av:elucidation"/>
        <IRI>designates</IRI>
