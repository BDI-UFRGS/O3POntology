    rdfs:subPropertyOf obo:BFO_0000216 ;
    skos:definition "b spatially projects onto c at all times =Def for all times t, b exists at t implies (b is a spatiotemporal region and c is a spatial region and c is the spatial extent of b at t)"@en ;
    skos:prefLabel "spatially projects onto at all times"@en .

obo:BFO_0000219 a owl:ObjectProperty ;
