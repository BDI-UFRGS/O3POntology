    owl:inverseOf Core:describes ;
    skos:example "some current event is described by the content of a newspaper article; some facility visit is described by the content of a visitor's log; some accident is described by the content of an accident report"@en ;
    AnnotationVocabulary:adaptedFrom "http://www.ontologyrepository.com/CommonCoreOntologies/Mid/InformationEntityOntology"^^xsd:anyURI ;
    AnnotationVocabulary:naturalLanguageDefinition "relation from an entity to an information content entity that characterizes the entity"@en .

Core:functionOf a owl:ObjectProperty ;
    rdfs:label "function of"@en,
        "função de"@pt-br ;
    rdfs:domain obo:BFO_0000034 ;
    rdfs:range obo:BFO_0000004 ;
    rdfs:subPropertyOf obo:BFO_0000197 ;
