obo:BFO_0000164 a owl:ObjectProperty ;
    rdfs:label "concretizes at all times"@en ;
    dc:identifier "257-BFO" ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( obo:BFO_0000015 obo:BFO_0000020 ) ] ;
    rdfs:range obo:BFO_0000031 ;
    rdfs:subPropertyOf obo:BFO_0000059 ;
    skos:definition "an s-dependent continuant b concretizes a g-dependent continuant c at all times =Def for all times t, b exists at t implies (c is the pattern or content which b shares at t with actual or potential copies)"@en ;
