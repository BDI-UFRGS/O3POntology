[] a owl:Axiom ;
    obo:IAO_0010000 <http://purl.obolibrary.org/obo/bfo/axiom/0000004> ;
    rdfs:comment "per discussion with Barry Smith" ;
    rdfs:seeAlso <http://www.referent-tracking.com/_RTU/papers/CeustersICbookRevised.pdf> ;
