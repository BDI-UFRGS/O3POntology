    rdfs:subPropertyOf obo:BFO_0000058 ;
    skos:definition "a g-dependent continuant c is concretized by an s-dependent continuant or process b at all times =Def for all times t, b exists at t implies (b concretizes c at t)"@en ;
    skos:prefLabel "is concretized by at all times"@en .

obo:BFO_0000166 a owl:ObjectProperty ;
    rdfs:label "participates in at all times"@en ;
    dc:identifier "251-BFO" ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( obo:BFO_0000020 obo:BFO_0000031 [ a owl:Class ;
