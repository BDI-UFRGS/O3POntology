        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#comment"/>
        <owl:annotatedTarget xml:lang="en">Relation between two things which have a fluid exchange connection</owl:annotatedTarget>
        <terms:source xml:lang="en">Flow Systems Ontology (FSO)</terms:source>
    </owl:Axiom>
    


