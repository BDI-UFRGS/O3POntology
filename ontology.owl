    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#Annulus -->


    <owl:Class rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#Annulus">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000029"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000050"/>
                <owl:someValuesFrom rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#Well"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <CommonCoreOntologies:definition xml:lang="en">def. a BFO:Site that occupies a well and is delimited by any void between any piping, tubing or casing and the piping, tubing, or casing immediately surrounding it</CommonCoreOntologies:definition>
        <rdfs:comment xml:lang="en">The annulus of an oil well or water well is any void between any piping, tubing or casing and the piping, tubing, or casing immediately surrounding it.</rdfs:comment>
        <rdfs:label xml:lang="en">annulus</rdfs:label>
        <rdfs:label xml:lang="pt-br">anular</rdfs:label>
    </owl:Class>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#Basin -->


    <owl:Class rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#Basin">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000146"/>
        <CommonCoreOntologies:definition xml:lang="en">def. a two-dimensional continuant faunal boundary</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="pt-br">bacia</rdfs:label>
        <rdfs:label xml:lang="en">basin</rdfs:label>
    </owl:Class>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#BeingOperatedCapability -->


    <owl:Class rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#BeingOperatedCapability">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/Capability"/>
        <CommonCoreOntologies:definition xml:lang="en">def. a capability to be operated</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="en">being operated capability</rdfs:label>
        <rdfs:label xml:lang="pt-br">capacidade de ser operado</rdfs:label>
    </owl:Class>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#Blower -->


    <owl:Class rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#Blower">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/MaterialArtifact"/>
        <CommonCoreOntologies:definition xml:lang="en">def. A material artifact designed to move gas with a moderate increase of pressure.</CommonCoreOntologies:definition>
        <rdfs:comment xml:lang="en">Blowers are used to move gas or air. They operate at a pressure ratio between that of a fan and a compressor.</rdfs:comment>
        <rdfs:label xml:lang="en">blower</rdfs:label>
        <rdfs:label xml:lang="pt-br">soprador</rdfs:label>
    </owl:Class>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#Borehole -->


    <owl:Class rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#Borehole">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000029"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000050"/>
                <owl:someValuesFrom rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#Well"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <oboInOwl:hasRelatedSynonym xml:lang="en">borehole</oboInOwl:hasRelatedSynonym>
        <oboInOwl:source xml:lang="en">https://www.lexico.com/en/definition/well</oboInOwl:source>
        <CommonCoreOntologies:definition xml:lang="en">def. a BFO:Site that is a boring in the Earth designed to bring oil, gas or water to the surface</CommonCoreOntologies:definition>
        <rdfs:comment xml:lang="en">A shaft sunk into the ground to obtain water, oil, or gas.</rdfs:comment>
        <rdfs:label xml:lang="pt-br">furo do poço</rdfs:label>
        <rdfs:label xml:lang="en">wellbore</rdfs:label>
    </owl:Class>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#BottomholePressure -->


    <owl:Class rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#BottomholePressure">
        <rdfs:subClassOf rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#Pressure"/>
        <rdfs:subClassOf>
            <owl:Restriction>
