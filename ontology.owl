            </owl:Restriction>
        </rdfs:subClassOf>
        <CommonCoreOntologies:definition xml:lang="en">def. a pressure in a control line</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="en">common close control line pressure</rdfs:label>
        <rdfs:label xml:lang="pt-br">pressão de linha de controle comum de fechamento</rdfs:label>
    </owl:Class>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#Condensate -->


    <owl:Class rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#Condensate">
