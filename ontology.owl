    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#componentOf -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#componentOf">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000050"/>
        <owl:inverseOf rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#hasComponent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <CommonCoreOntologies:definition xml:lang="en">def. a part of relation that holds between two material entities</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="en">component of</rdfs:label>
        <rdfs:label xml:lang="pt-br">componente de</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#connectedTo -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#connectedTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <rdfs:label xml:lang="pt-br">conectado a</rdfs:label>
        <rdfs:label xml:lang="en">connected to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#exchangesFluidWith -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#exchangesFluidWith">
        <rdfs:subPropertyOf rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#connectedTo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <rdfs:comment xml:lang="en">Relation between two things which have a fluid exchange connection</rdfs:comment>
        <rdfs:label xml:lang="en">exchanges fluid with</rdfs:label>
        <rdfs:label xml:lang="pt-br">troca fluido com</rdfs:label>
        <AnnotationVocabulary:adaptedFrom xml:lang="en">http://www.w3id.org/fso</AnnotationVocabulary:adaptedFrom>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#feedsFluidTo -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#feedsFluidTo">
        <rdfs:subPropertyOf rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#exchangesFluidWith"/>
        <owl:inverseOf rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#hasFluidFedBy"/>
        <rdfs:comment xml:lang="en">Relation from a thing feeding fluid to the thing it is feeding fluid to.</rdfs:comment>
        <rdfs:label xml:lang="pt-br">alimenta fluido para</rdfs:label>
        <rdfs:label xml:lang="en">feeds fluid to</rdfs:label>
        <AnnotationVocabulary:adaptedFrom xml:lang="en">http://www.w3id.org/fso</AnnotationVocabulary:adaptedFrom>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#hasComponent -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#hasComponent">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000051"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label xml:lang="en">has component</rdfs:label>
        <rdfs:label xml:lang="pt-br">tem componente</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#hasFluidFedBy -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#hasFluidFedBy">
        <rdfs:subPropertyOf rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#exchangesFluidWith"/>
        <rdfs:comment xml:lang="en">Relation from a thing that is fed fluid to the thing feeding the fluid.</rdfs:comment>
        <rdfs:label xml:lang="en">has fluid fed by</rdfs:label>
        <rdfs:label xml:lang="pt-br">tem fluido alimentado por</rdfs:label>
        <AnnotationVocabulary:adaptedFrom xml:lang="en">http://www.w3id.org/fso</AnnotationVocabulary:adaptedFrom>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#hasFluidReturnedBy -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#hasFluidReturnedBy">
        <rdfs:subPropertyOf rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#feedsFluidTo"/>
        <owl:inverseOf rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#returnsFluidTo"/>
        <rdfs:comment xml:lang="en">Relation between a thing that has fluid returned to it and the thing returning the fluid.</rdfs:comment>
        <rdfs:label xml:lang="en">has fluid returned by</rdfs:label>
        <rdfs:label xml:lang="pt-br">tem fluido retornado por</rdfs:label>
        <AnnotationVocabulary:adaptedFrom xml:lang="en">http://www.w3id.org/fso</AnnotationVocabulary:adaptedFrom>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#hasFluidSuppliedBy -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#hasFluidSuppliedBy">
        <rdfs:subPropertyOf rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#feedsFluidTo"/>
        <owl:inverseOf rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#suppliesFluidTo"/>
        <rdfs:comment xml:lang="en">Relation between a thing that has fluid supplied to it and the thing supplying the fluid.</rdfs:comment>
        <rdfs:label xml:lang="en">has fluid supplied by</rdfs:label>
        <rdfs:label xml:lang="pt-br">tem fluido fornecido por</rdfs:label>
        <AnnotationVocabulary:adaptedFrom xml:lang="en">http://www.w3id.org/fso</AnnotationVocabulary:adaptedFrom>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#injects -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#injects">
        <owl:inverseOf rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#isInjectedBy"/>
        <CommonCoreOntologies:definition xml:lang="en">def. to introduce (something) into a passage, cavity, or solid material under pressure</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="en">injects</rdfs:label>
        <rdfs:label xml:lang="pt-br">injeta</rdfs:label>
        <rdfs:seeAlso xml:lang="en">https://www.lexico.com/en/definition/inject</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#isInjectedBy -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#isInjectedBy">
        <CommonCoreOntologies:definition xml:lang="en">def. inverse of injects</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="en">is injected by</rdfs:label>
        <rdfs:label xml:lang="pt-br">é injetado por</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#isProducedBy -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#isProducedBy">
        <CommonCoreOntologies:definition xml:lang="en">def. inverse of produces</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="en">is produced by</rdfs:label>
        <rdfs:label xml:lang="pt-br">é produzido por</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#isSeparatedBy -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#isSeparatedBy">
        <CommonCoreOntologies:definition xml:lang="en">def. inverse of separates</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="en">is separated by</rdfs:label>
        <rdfs:label xml:lang="pt-br">é separado por</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#isTreatedBy -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#isTreatedBy">
        <owl:inverseOf rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#treats"/>
        <CommonCoreOntologies:definition xml:lang="en">def. inverse of treats</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="en">is treated by</rdfs:label>
        <rdfs:label xml:lang="pt-br">é tratado por</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#measures -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#measures">
        <rdfs:domain rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#Sensor"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000019"/>
                    <rdf:Description rdf:about="https://spec.industrialontologies.org/ontology/core/Core/ProcessCharacteristic"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <CommonCoreOntologies:definition xml:lang="en">def. a relation between a sensor and a quality that the sensor is designed to measure</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="en">measures</rdfs:label>
        <rdfs:label xml:lang="pt-br">mede</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#returnsFluidTo -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#returnsFluidTo">
        <rdfs:subPropertyOf rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#hasFluidFedBy"/>
        <rdfs:comment xml:lang="en">Relation implying logical return flow from a thing to another.</rdfs:comment>
        <rdfs:label xml:lang="pt-br">retorna fluido para</rdfs:label>
        <rdfs:label xml:lang="en">returns fluid to</rdfs:label>
        <AnnotationVocabulary:adaptedFrom xml:lang="en">http://www.w3id.org/fso</AnnotationVocabulary:adaptedFrom>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#suppliesFluidTo -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#suppliesFluidTo">
        <rdfs:subPropertyOf rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#hasFluidFedBy"/>
        <rdfs:comment xml:lang="en">Relation implying logical supply flow from a thing to another.</rdfs:comment>
        <rdfs:label xml:lang="pt-br">fornece fluido para</rdfs:label>
        <rdfs:label xml:lang="en">supplies fluid to</rdfs:label>
        <AnnotationVocabulary:adaptedFrom xml:lang="en">http://www.w3id.org/fso</AnnotationVocabulary:adaptedFrom>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#transports -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#transports">
        <CommonCoreOntologies:definition xml:lang="en">def. the action of transporting something or the state of being transported</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="pt-br">transporta</rdfs:label>
        <rdfs:label xml:lang="en">transports</rdfs:label>
        <rdfs:seeAlso xml:lang="en">https://www.lexico.com/en/definition/transport</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#treats -->


    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#treats">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <CommonCoreOntologies:definition xml:lang="en">def. to apply a process or a substance to (something) to protect or preserve it or to give it particular properties</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="pt-br">trata</rdfs:label>
        <rdfs:label xml:lang="en">treats</rdfs:label>
        <rdfs:seeAlso xml:lang="en">https://www.lexico.com/en/definition/treat</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#hasFlowRateValue -->


    <owl:DatatypeProperty rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#hasFlowRateValue">
        <rdfs:domain rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#FlowRate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <CommonCoreOntologies:definition xml:lang="en">def. a relation between a flow rate and a float value representing the volumetric flow magnitude</CommonCoreOntologies:definition>
        <rdfs:comment xml:lang="en">Flow rate typically expressed in cubic meters per day (m³/d) or barrels per day (bbl/d).</rdfs:comment>
        <rdfs:label xml:lang="en">has flow rate value</rdfs:label>
        <rdfs:label xml:lang="pt-br">tem valor de vazão</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
