        <owl:disjointWith rdf:resource="https://www.inf.ufrgs.br/ontologies/o3po#Water"/>
        <oboInOwl:source xml:lang="en">https://www.mckinseyenergyinsights.com/resources/refinery-reference-desk/condensate/</oboInOwl:source>
        <CommonCoreOntologies:definition xml:lang="en">def. crude oil that is a mixture of light liquid hydrocarbons, similar to a very light crude oil</CommonCoreOntologies:definition>
        <rdfs:comment xml:lang="en">Condensate is a mixture of light liquid hydrocarbons, similar to a very light (high API) crude oil. It is typically separated out of a natural gas stream at the point of production (field separation) when the temperature and pressure of the gas is dropped to atmospheric conditions.</rdfs:comment>
        <rdfs:label xml:lang="pt-br">condensado</rdfs:label>
        <rdfs:label xml:lang="en">condensate</rdfs:label>
    </owl:Class>
    


    <!-- https://www.inf.ufrgs.br/ontologies/o3po#ControlLine -->


    <owl:Class rdf:about="https://www.inf.ufrgs.br/ontologies/o3po#ControlLine">
